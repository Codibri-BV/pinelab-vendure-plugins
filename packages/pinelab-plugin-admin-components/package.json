{
  "name": "@pinelab/pinelab-plugin-admin-components",
  "version": "1.0.0",
  "description": "Vendure plugin that provides a unified user interface for both the invoice and picklist plugins",
  "icon": "receipt",
  "author": "Surafel Tariku <surafelmelese09@gmail.com>",
  "homepage": "https://pinelab-plugins.com/",
  "repository": "https://github.com/Pinelab-studio/pinelab-vendure-plugins",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "serve": "yarn start && nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node test/dev-server.ts\"",
    "start": "ts-node test/dev-server.ts",
    "build": "rimraf dist && tsc && copyfiles -u 1 'src/ui/**/*' dist/",
    "test": "vitest run",
    "generate": "graphql-codegen"
  },
  "dependencies": {
    "pdf-creator-node": "2.3.4",
    "tmp": "0.2.1"
  }
}
