{
  "name": "@pinelab/vendure-plugin-goedgepickt",
  "version": "0.0.4",
  "description": "Vendure plugin for integration with the Goedgepickt order picking platform",
  "author": "Martijn van de Brug <martijn@pinelab.studio>",
  "homepage": "https://pinelab-plugins.com/",
  "repository": "https://github.com/Pinelab-studio/pinelab-vendure-plugins",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/vendure-plugin-goedgepickt/src/index.js",
  "types": "dist/vendure-plugin-goedgepickt/src/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "serve": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node test/dev-server.ts\"",
    "start": "ts-node test/dev-server.ts",
    "build": "rimraf dist && yarn graphql-codegen && tsc && copyfiles -u 1 'src/ui/**/*' dist/vendure-plugin-goedgepickt/src/",
    "test": "vitest run"
  },
  "dependencies": {
    "node-fetch": "2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@swc/core": "^1.3.59",
    "@vendure/admin-ui-plugin": "2.0.4",
    "@vendure/asset-server-plugin": "2.0.4",
    "@vendure/core": "2.0.4",
    "@vendure/testing": "2.0.4",
    "@vendure/ui-devkit": "2.0.4",
    "copyfiles": "^2.4.1",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "4.9.5",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.30.1"
  },
  "gitHead": "476f36da3aafea41fbf21c70774a30306f1d238f"
}
