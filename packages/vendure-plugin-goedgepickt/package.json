{
  "name": "vendure-plugin-goedgepickt",
  "version": "0.2.3",
  "description": "Vendure plugin for integration with the Goedgepickt order picking platform",
  "author": "Martijn van de Brug <martijn@pinelab.studio>",
  "homepage": "https://pinelab.studio",
  "repository": "https://github.com/martijnvdbrug/pinelab-vendure-plugins",
  "license": "MIT",
  "private": false,
  "main": "dist/vendure-plugin-goedgepickt/src/index.js",
  "types": "dist/vendure-plugin-goedgepickt/src/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "serve": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node test/dev-server.ts\"",
    "start": "ts-node test/dev-server.ts",
    "build": "rimraf dist && tsc && yarn graphql-codegen && copyfiles -u 1 'src/ui/**/*' dist/vendure-plugin-goedgepickt/src/",
    "test": "jest --preset=\"ts-jest\""
  },
  "dependencies": {
    "node-fetch": "2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@types/jest": "^27.4.0",
    "@types/localtunnel": "^2.0.1",
    "@vendure/admin-ui": "1.4.2",
    "@vendure/admin-ui-plugin": "1.4.2",
    "@vendure/asset-server-plugin": "1.4.2",
    "@vendure/core": "1.4.3",
    "@vendure/testing": "^1.4.3",
    "@vendure/ui-devkit": "1.4.2",
    "copyfiles": "^2.4.1",
    "jest": "^27.4.7",
    "localtunnel": "^2.0.2",
    "nock": "^13.2.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.2",
    "ts-node": "^9.1.1"
  }
}
