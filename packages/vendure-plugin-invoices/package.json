{
  "name": "vendure-plugin-invoices",
  "version": "0.0.1",
  "description": "Vendure plugin for invoice generation",
  "author": "Martijn van de Brug <martijn@pinelab.studio>",
  "homepage": "https://pinelab.studio",
  "repository": "https://github.com/martijnvdbrug/pinelab-vendure-plugins",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "yarn ts-node test/dev-server.ts",
    "serve": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node test/dev-server.ts\"",
    "build": "rimraf dist && yarn graphql-codegen && tsc && copyfiles -u 1 'src/ui/**/*' dist/",
    "test": "jest --preset=\"ts-jest\""
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "pdf-creator-node": "^2.3.4",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@google-cloud/storage": "^5.18.2",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@types/adm-zip": "^0.4.34",
    "@types/jest": "^27.0.1",
    "@types/tmp": "^0.2.3",
    "@vendure/admin-ui": "1.4.3",
    "@vendure/admin-ui-plugin": "1.4.3",
    "@vendure/core": "1.4.3",
    "@vendure/testing": "1.4.3",
    "@vendure/ui-devkit": "1.4.3",
    "copyfiles": "^2.4.1",
    "jest": "^27.2.0",
    "localtunnel": "^2.0.1",
    "nock": "^13.1.3",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "typescript": "4.3.5"
  }
}
