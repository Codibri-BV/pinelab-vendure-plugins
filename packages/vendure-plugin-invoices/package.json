{
  "name": "@pinelab/vendure-plugin-invoices",
  "version": "0.0.2",
  "description": "Vendure plugin for invoice generation",
  "author": "Martijn van de Brug <martijn@pinelab.studio>",
  "homepage": "https://pinelab-plugins.com/",
  "repository": "https://github.com/Pinelab-studio/pinelab-vendure-plugins",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "yarn ts-node test/dev-server.ts",
    "serve": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node test/dev-server.ts\"",
    "build": "rimraf dist && yarn graphql-codegen && tsc && copyfiles -u 1 'src/ui/**/*' dist/",
    "test": "vitest run"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "pdf-creator-node": "^2.3.4",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@google-cloud/storage": "^5.18.2",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@swc/core": "^1.3.59",
    "@types/adm-zip": "^0.4.34",
    "@types/tmp": "^0.2.3",
    "@vendure/admin-ui-plugin": "2.0.4",
    "@vendure/core": "2.0.4",
    "@vendure/testing": "2.0.4",
    "@vendure/ui-devkit": "2.0.4",
    "aws-sdk": "^2.1099.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.30.1"
  },
  "gitHead": "476f36da3aafea41fbf21c70774a30306f1d238f"
}
