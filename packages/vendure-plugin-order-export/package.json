{
  "name": "vendure-plugin-order-export",
  "version": "5.0.0",
  "description": "Vendure plugin for exporting orders to a file",
  "author": "Martijn van de Brug <martijn@pinelab.studio>",
  "homepage": "https://pinelab.studio",
  "repository": "https://github.com/Pinelab-studio/pinelab-vendure-plugins",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "serve": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node test/dev-server.ts\"",
    "start": "ts-node test/dev-server.ts",
    "build": "rimraf dist && tsc && copyfiles -u 1 'src/ui/**/*' dist/",
    "test": "jest --preset=\"ts-jest\" --forceExit"
  },
  "dependencies": {
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@vendure/admin-ui-plugin": "1.9.2",
    "@vendure/core": "1.9.2",
    "@vendure/testing": "1.9.2",
    "@vendure/ui-devkit": "1.9.2",
    "copyfiles": "^2.4.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0"
  }
}
